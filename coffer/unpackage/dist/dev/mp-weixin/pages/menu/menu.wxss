.mask{
	 background:rgba(0,0,0,.3);
	 position:fixed;
	 z-index:999;
	 top:0;
	 right:0;
	 bottom:0;
	 left:0;
	 width:100%;
}
.popup{
	 position:fixed;
	width:690rpx;
	height:76%;
	background:#fff;
	-webkit-box-shadow:0 0 30rpx rgba(0,0,0,.1);
	        box-shadow:0 0 30rpx rgba(0,0,0,.1);
	top:12%;
	left:30rpx;
}
.popup-header{
	  position:relative;
	  height:33%;
	 background:red;
	background:url(https://s2.luckincoffeecdn.com/luckywebrm/images/index/cooperation/part5_picture2.png) center;
}
.popup-title,.popup-en{
		color:#fff;
		margin-left:20rpx;
		position:absolute;
}
.popup-title{
    	font-size:36rpx;
		bottom:42rpx;
}
.popup-en{
		font-size:22rpx;
	    bottom:15rpx;
}
.popup-close{
		color:#fff;
		position:absolute;
		top:20rpx;
		right:20rpx;
		font-size:55rpx;
}
.popup-middle{
		height:40%;
		overflow:auto;
		-webkit-box-sizing: border-box;
		        box-sizing: border-box;
		padding:10rpx 30rpx 0;
}
.sort{
	  display:-webkit-box;
	  display:-webkit-flex;
	  display:-ms-flexbox;
	  display:flex;
	  font-size: 24rpx;
	  margin-top: 30rpx;
}
.sort-lable{
		margin-right:20rpx;
}
.sort-select{
		width:120rpx;
		border-radius: 30rpx;
		border: 1px solid #e3dbd3;
		color: #e3dbd3;
		text-align: center;
		margin-left:20rpx;
}
.sort-select.active{
		background:#e3dbd3;
		color:#fff;
		font-weight:400;
}
.describe{
		border-top: 1px solid #f1f1f1;margin-top: 30rpx;padding: 30rpx 0 10rpx;
}
.describe-title{
	 margin-top:10rpx;
}
.describe-text{
	  color:#666;
	  font-size:24rpx;
}
.popup-footer{
	border-top: 1px solid #f1f1f1;
	padding:20rpx 10rpx 0;
	height:232rpx;
}
.popup-priceout{
		display: -webkit-box;
		display: -webkit-flex;
		display: -ms-flexbox;
		display: flex;
		-webkit-box-align:center;
		-webkit-align-items:center;
		    -ms-flex-align:center;
		        align-items:center;
		 width:96%;
		 margin:0 auto;
		 -webkit-box-pack: justify;
		 -webkit-justify-content: space-between;
		     -ms-flex-pack: justify;
		         justify-content: space-between;
}
.popup-price{
	 padding-left:10rpx;
}
.text-main{
	 font-size:32rpx;
	 color:#222;
}
.text-small{
     font-size:20rpx;
	 color:#666;
	 font-size:24rpx;
}
.popup-numbox{
	 display:-webkit-box;
	 display:-webkit-flex;
	 display:-ms-flexbox;
	 display:flex;
	 -webkit-box-align:center;
	 -webkit-align-items:center;
	     -ms-flex-align:center;
	         align-items:center;
}
.numbox-minus,.numbox-plus{
		font-size:56rpx;
		line-height:68rpx;
		color:#91b5d9;
}
.numbox-value{
	 margin:0 16rpx;
	 color:#91b5d9;
	font-weight:700;
	min-width:20rpx;
}
.btn-group{
		display:-webkit-box;
		display:-webkit-flex;
		display:-ms-flexbox;
		display:flex;
	    margin-top:10rpx;
		-webkit-box-pack:end;
		-webkit-justify-content:flex-end;
		    -ms-flex-pack:end;
		        justify-content:flex-end;
		border-top: 1px solid #f1f1f1;
		padding-top:20rpx;
		width:100%;
}
.btn{
		height:60rpx;
		text-align: center;
	    color:#fff;
		margin-right:14rpx;
	    line-height:60rpx;	
		font-size:24rpx;
	    padding:0;
}
.first-btn{
	   width:206rpx;
	   border:1px solid #dc5a00;
	   background:#e06e11;
}
.two-btn{
	 border:1px solid #73a1cf;
	 width:170rpx;
	 background:#f3f3f3;
	color:#73a1cf;
}
.three-btn{
	width:180rpx;
	border:1px solid #6c9ccd;
	background:#79a1cf;
}
	/*menu yinchang-end*/
	/*menu main-start*/
.container{border-top: 1px solid #f0f0f0;}
.page-body{
		display:-webkit-box;
		display:-webkit-flex;
		display:-ms-flexbox;
		display:flex;
}
.active{
		 background:#fff;
		 font-weight:700;
		 color:#333;
}
.nav-left{
      width:180rpx;
	 background:#f7f7f7;
}
.nav-left-item{
	height:86rpx;
	border-bottom:1px solid #ececec;
	font-size:28rpx;
	display:-webkit-box;
	display:-webkit-flex;
	display:-ms-flexbox;
	display:flex;
	-webkit-box-pack:center;
	-webkit-justify-content:center;
	    -ms-flex-pack:center;
	        justify-content:center;
	color:#666;
	line-height:86rpx;
}
.nav-left-item:last-child{
	  border-bottom:none;
}
.nav-right{
	  width:570rpx;
	 background:#fff;
}
.nav-right-item{
	 width:100%;	
	 font-size:28rpx;
	 padding:0 26rpx;
	 -webkit-box-sizing: border-box;
	         box-sizing: border-box;
}
.item-header{
		padding-top:36rpx;
		font-size:24rpx;
		font-weight:700;
		line-height:24rpx;
}
.item-state{
		margin-top:10rpx;
}
.item-state,.item-en,.item-default{
		font-size:24rpx;
		line-height:20rpx;
		color:#999;
}
.item-li{padding: 22rpx 0;position: relative;border-bottom: 1px solid #f1f1f1;height: 134rpx;}
.item-img{width: 134rpx;height: 134rpx;border-radius: 8rpx;}
.item-text{display: inline-block;vertical-align: top;margin-left: 16rpx;}
.item-title{font-size: 28rpx;line-height: 28rpx;font-weight: 700;}
.item-en{margin: 10rpx 0 8rpx 0;}
.item-price{font-size: 28rpx;line-height: 28rpx;font-weight: 700;margin-top: 28rpx;}
.plus-filled{position: absolute;bottom: 16rpx;right: 0;color: #81aad2;}
.fixed{position: fixed;top: 0;left: 206rpx;background: #fff;}
